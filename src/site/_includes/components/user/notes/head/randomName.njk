<script>
  const names = ["Jack", "Jill", "Jake", "Jordan", "Jillian", "Juliette"];
  const random = Math.floor(Math.random() * names.length);

  document.addEventListener('DOMContentLoaded', function() {
    const nodeList = document.querySelectorAll(".randName");
    for (let i=0; i< nodeList.length; i++) {
      nodeList[i].textContent = names[random];
    }
  });
</script>
